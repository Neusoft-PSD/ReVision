function imHandle = projectionprofile_image(im, profiles, xLocs,xPeaks, yLocs, yPeaks)
    imHandle = figure;
    subplot(2,3,1);
    imshow(im);

    subplot(2,3,2);
    plot(profiles.x);
    title('x profile');

    subplot(2,3,3);
    plot(profiles.y);
    title('y profile');

    subplot(2,3,5);
    plot(xLocs, xPeaks, 'ro','MarkerFaceColor','red','MarkerSize',2);

    subplot(2,3,6);
    plot(yLocs, yPeaks, 'ro','MarkerFaceColor','red','MarkerSize',2);

end